/**index.wxss**/
.container1{
  width:100%; 
} 

.container1 image{
  width:100%;
}

.lhb{
  width:54px;height:54px;
  position: fixed;
  right:0px;
  top:45%;
}

.lhb image{
  width:100%;
  height:100%;
}

button.zx{
  width:44px;
  height:44px;
  position: fixed;
  right:0px;
  top:60%;
  background:transparent;  
}

.zx image{
  display:inline-block;
  position: absolute;
  top:0px;
  left:0px;
  width:44px;
  height:44px;
}

.show-btn {
  margin-top: 100rpx;
  color: #22cc22;
}
.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}
.modal-dialog {
  width: 540rpx;
  height:760rpx;  
  position: fixed;
  top: 50%;
  left: 50%;
  z-index: 9999;
  margin-left:-270rpx;
  margin-top:-380rpx;
  border-radius: 30rpx;
  background:url('data:image/jpg;base64,') no-repeat center;
  background-size:100%;
  
}
.modal-title {
  padding-top: 50rpx;
  font-size: 38rpx;
  color: #333;
  text-align: center;
}

.modal-desp{
  font-size:24rpx;
  color:#888;
  padding:20rpx 0px;
}

.modal-content {
  width:70%;
  padding-left:15%;
  position:absolute;
  bottom:200rpx;
}
.modal-input {
  display: flex;
  background: #fff;
  border: 2rpx solid #ddd;
  border-radius: 4rpx;
  font-size: 28rpx;
}

.input {
  width: 100%;
  height: 82rpx;
  font-size: 28rpx;
  line-height: 28rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
  color: #333;
}
input-holder {
  color: #666;
  font-size: 28rpx;
}

.modal-footer {
  width:100%;
  display: flex;
  flex-direction: row;
  height: 86rpx;
  font-size: 34rpx;
  line-height: 86rpx;
  position:absolute;
  bottom:40rpx;
}
.btn-cancel {
  width: 80rpx;
  height:80rpx;
  background:url('data:image/png;base64,') no-repeat center;
  z-index:9999;
  background-size:100%;
  text-align: center;
  position: absolute;
  right:-36rpx;
  top:-36rpx;
}
.btn-confirm {
  width: 100%;
  color: #e93332;
  text-align: center;
  background:url('data:image/png;base64,') no-repeat center;
  background-size:137px 40px;
}